 /* 动画效果 */
 .animationBox{
    animation:bounceIn 0.75s linear;
  }
@keyframes bounceIn {
  0% {
    opacity: 0;
      transform: scale3d(.3, .3, .3);
  }

  20% {
      transform: scale3d(0.6, 0.6, 0.6);
  }

  40% {
      transform: scale3d(.9, .9, .9);
  }

  60% {
    opacity: 1;
      transform: scale3d(1.03, 1.03, 1.03);
  }

  80% {
      transform: scale3d(.97, .97, .97);
  }

  100% {
    opacity: 1;
       transform: scale3d(1, 1, 1);
  }
}


.animationCart{
   animation:bounceInLeft 0.8s linear;
}

@keyframes bounceInLeft {

  0% {
    opacity: 0;
    -webkit-transform: translate3d(0,-1000rpx,0);
            transform: translate3d(0,-1000rpx,0);
  }

  60% {
    opacity: 1;
    -webkit-transform: translate3d(0,50rpx,0);
            transform: translate3d(0,50rpx,0);
  }

  75% {
    -webkit-transform: translate3d(0,-80rpx,0);
            transform: translate3d(0,-80rpx,0);
  }

  90% {
    -webkit-transform: translate3d(0,30rpx,0);
            transform: translate3d(0,30rpx,0);
  }

  100% {
    -webkit-transform: none;
            transform: none;
  }
}

